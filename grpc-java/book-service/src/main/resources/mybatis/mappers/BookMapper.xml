<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bookstore.dao.mapper.BookMapper">
    <select id="getBookByIdMybatis" resultType="com.bookstore.dao.Book">
        SELECT * FROM book
        WHERE book_id = #{id}
    </select>
</mapper>